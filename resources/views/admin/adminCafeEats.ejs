<div class="container mx-auto mt-6">
  <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4">
    <% orders.forEach(order => { %>
      <div class="bg-white rounded-md shadow-md p-4">
        <p class="text-lg font-semibold mb-2">Order ID: <%= order._id %></p>
        <p class="text-gray-600 mb-2">Customer: <%= order.customerName %></p>
        <p class="text-gray-600 mb-2">Total Price: ₹<%= order.totalPrice %></p>
        <!-- Add more order details as needed -->
        <a href="/admin/orderDetails/<%= order._id %>" class="text-blue-500 hover:underline">View Details</a>
      </div>
    <% }); %>
  </div>
</div>




      


      




